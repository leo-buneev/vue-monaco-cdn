{
  "name": "vue-monaco-cdn",
  "version": "2.0.1",
  "description": "Lightweight MonacoEditor component for Vue.js using CDN",
  "repository": {
    "url": "https://github.com/leo-buneev/vue-monaco-cdn/",
    "type": "git"
  },
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve ./example/main.js --open",
    "build": "vue-cli-service build --name vue-monaco-cdn --entry ./src/index.js --target lib",
    "lint": "vue-cli-service lint",
    "prepublishOnly": "npm run lint && npm run build",
    "test": "vue-cli-service test:unit"
  },
  "dependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@vue/cli-plugin-babel": "^5.0.0-rc.1",
    "@vue/cli-plugin-eslint": "^5.0.0-rc.1",
    "@vue/cli-plugin-unit-jest": "^5.0.0-rc.1",
    "@vue/cli-service": "^5.0.0-rc.1",
    "@vue/compiler-sfc": "^3.2.26",
    "@vue/eslint-config-prettier": "^5.0.0-rc.1",
    "@vue/test-utils": "^1.0.0-beta.20",
    "@vuedoc/md": "^1.3.3",
    "babel-jest": "^23.0.1",
    "dedent": "^0.7.0",
    "eslint": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "vue-cli-plugin-component": "^1.7.1"
  },
  "files": [
    "dist/*.css",
    "dist/*.map",
    "dist/*.js",
    "src/*"
  ],
  "keywords": [
    "vue",
    "component",
    "monaco",
    "monaco-editor"
  ],
  "author": "leo-buneev <buneev.leonid@gmail.com>",
  "license": "MIT",
  "main": "dist/vue-monaco-cdn.umd.js",
  "module": "dist/vue-monaco-cdn.common.min.js"
}
